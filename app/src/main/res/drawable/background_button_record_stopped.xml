<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid
                android:color="@color/colorWindowGreyBackground"/>
            <size
                android:width="64dp"
                android:height="64dp"/>
        </shape>
    </item>

    <item
        android:bottom="16dp"
        android:left="16dp"
        android:right="16dp"
        android:top="16dp">
        <shape
            android:shape="oval">
            <solid android:color="@android:color/holo_red_dark"/>
        </shape>
    </item>
</layer-list>