<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <stroke
                android:width="2dp"
                android:color="@android:color/white"/>
            <size
                android:width="48dp"
                android:height="48dp"/>
        </shape>
    </item>

    <item
        android:bottom="8dp"
        android:left="8dp"
        android:right="8dp"
        android:top="8dp">
        <shape
            android:shape="oval">
            <solid android:color="@android:color/white"/>
        </shape>
    </item>
</layer-list>